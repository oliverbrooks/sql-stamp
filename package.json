{
  "name": "sql-stamp",
  "version": "0.5.0",
  "bin": {
    "sql-stamp": "./bin/cli.js"
  },
  "scripts": {
    "test": "mocha test/index.js",
    "coverage": "mocha test/index.js -r blanket -R mocha-term-cov-reporter",
    "coverage-ci": "mocha test/index.js -r blanket -R mocha-lcov-reporter",
    "benchmark": "node test/benchmark/index.js"
  },
  "config": {
    "blanket": {
      "pattern": [
        "index.js"
      ],
      "data-cover-never": "node_modules"
    }
  },
  "dependencies": {
    "bluebird": "^2.9.25",
    "glob": "^5.0.6",
    "yargs": "^3.9.0"
  },
  "devDependencies": {
    "blanket": "^1.1.7",
    "coveralls": "^2.11.2",
    "lodash": "^3.9.1",
    "mocha": "^2.2.4",
    "mocha-lcov-reporter": "0.0.2",
    "mocha-term-cov-reporter": "^0.2.0"
  }
}
