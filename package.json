{
  "name": "sql-stamp",
  "description": "The tiny SQL templater",
  "version": "0.6.3",
  "scripts": {
    "test": "mocha test/index.js",
    "coverage": "mocha test/index.js -r blanket -R mocha-term-cov-reporter",
    "coverage-ci": "mocha test/index.js -r blanket -R mocha-lcov-reporter > lcov.info && codeclimate < lcov.info",
    "benchmark": "node test/benchmark/no-pre-compile.js; node test/benchmark/pre-compile.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/orangemug/sql-stamp.git"
  },
  "config": {
    "blanket": {
      "pattern": [
        "index.js"
      ],
      "data-cover-never": "node_modules"
    }
  },
  "keywords": [
    "template",
    "sql"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "devDependencies": {
    "blanket": "^1.1.7",
    "codeclimate-test-reporter": "^0.3.2",
    "coveralls": "^2.11.2",
    "mocha": "^2.2.4",
    "mocha-lcov-reporter": "0.0.2",
    "mocha-term-cov-reporter": "^0.2.3"
  },
  "dependencies": {
    "bluebird": "^2.9.27",
    "lodash.assign": "^4.0.9"
  }
}
